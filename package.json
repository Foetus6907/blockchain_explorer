    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json"
  },
  "dependencies": {
    "@quasar/extras": "^1.13.0",
    "quasar": "^2.5.5",
    "typescript": "^4.6.2",
    "vite-plugin-html": "^2.1.1",
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.0.5",
    "@vitejs/plugin-vue": "^1.9.3",
    "prettier": "^2.3.2",
    "sass": "^1.32.0",
    "vite": "^2.6.4"
  }
}

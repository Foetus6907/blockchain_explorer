{
  "name": "coding_test",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "start": "vite --host",
    "build": "vite build",
    "serve": "vite preview",
    "clean": "rm -rf .blocklet",
    "bundle": "npm run clean && vite build --base /.blocklet/proxy/z8iZnDYcvztvMywpyvTjbLwiy57Bq25xcLAbd && blocklet bundle --zip --create-release",
    "deploy": "npm run bundle && blocklet deploy .blocklet/bundle",
    "upload": "npm run bundle && blocklet upload .blocklet/release/blocklet.json",
    "test": "jest tests --config jest.setup.js"
  },
  "dependencies": {
    "@quasar/extras": "^1.13.0",
    "quasar": "^2.5.5",
    "typescript": "^4.6.2",
    "vite-plugin-html": "^2.1.1",
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "@quasar/quasar-app-extension-testing-unit-jest": "^2.2.3",
    "@quasar/vite-plugin": "^1.0.5",
    "@types/jest": "^27.4.1",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/test-utils": "^2.0.0-rc.18",
    "jest": "^26.6.3",
    "prettier": "^2.3.2",
    "sass": "^1.32.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.7.0",
    "vite": "^2.6.4",
    "vue-jest": "^5.0.0-alpha.10"
  }
}

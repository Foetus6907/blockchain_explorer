{
  "name": "coding_test",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "start": "vite --host",
    "build": "vite build",
    "serve": "vite preview",
    "clean": "rm -rf .blocklet",
    "bundle": "npm run clean && vite build --base /.blocklet/proxy/z8iZnDYcvztvMywpyvTjbLwiy57Bq25xcLAbd && blocklet bundle --zip --create-release",
